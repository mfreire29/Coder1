  console.log(respuesta)\n        })\n        .catch(()=>{\n            console.log('Errorrrr')\n        })\n    },[cantidad])\n\n    const aumentarCantidad = () => {\n        if(stock > 0 && cantidad < stock) {\n            setCantidad(cantidad+1)\n        }\n    }\n    \n    const reducirCantidad = () => {\n        if(cantidad > initial) {\n            setCantidad(cantidad-1)\n        }\n    }\n\n    const onAdd = () => {\n        if(cantidad === 1) {\n            console.log(\"Se ha agregado \" + cantidad + \" producto al carrito.\" )\n        } else {\n            console.log(\"Se han agregado \" + cantidad + \" productos al carrito.\" )\n        }\n    }\n\n\n\n    return (\n        <>\n\n            <div className=\"item-count mx-auto\">\n                <button className=\"btn btn-dark left\" onClick = { reducirCantidad }>-</button>  \n                <span>{stock > 0 ? cantidad : 0}</span>\n                <button className=\"btn btn-dark right\" onClick = { aumentarCantidad }>+</button>\n            </div>\n            {stock > 0 ? \n                <button className=\"btn btn-dark add\" onClick = { onAdd }>Agregar al Carrito</button>    \n                :\n                <button className=\"btn btn-dark add\">SIN STOCK</button>\n            }\n        </>\n    )\n}\n\nexport default ItemCount\n","/home/mau/Escritorio/CursoReact/demo/src/components/ItemList.js",[],"/home/mau/Escritorio/CursoReact/demo/src/components/Item.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":2,"column":8,"nodeType":"71","messageId":"72","endLine":2,"endColumn":17},{"ruleId":"69","severity":1,"message":"73","line":3,"column":8,"nodeType":"71","messageId":"72","endLine":3,"endColumn":16},{"ruleId":"74","severity":1,"message":"75","line":9,"column":11,"nodeType":"76","endLine":9,"endColumn":48},{"ruleId":"74","severity":1,"message":"75","line":26,"column":17,"nodeType":"76","endLine":26,"endColumn":70},{"r