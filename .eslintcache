>\n                    \n                    {\n                        notif ?\n                            estado > 0 ?\n                                estado === 1  ?\n                                    <p className=\"mt-3 text-center p-0\">Se ha agregado { estado } al carrito.</p>\n                                :\n                                    <p className=\"mt-3 text-center p-0\">Se han agregado { estado } al carrito.</p>\n                            :\n                                ''\n                        :\n                            ''\n                    }\n                    {\n                        isInCart(id).length > 0 ?\n                            <>\n                                <NavLink to=\"/cart\" className=\"nav-link text-center m-0\" >\n                                    <button className=\"btn btn-dark add\" >Terminar Compra</button>\n                                </NavLink>\n                                <button className=\"btn btn-dark add mt-0\" onClick={eliminarProductoCarrito}>Eliminar del Carro</button>\n                            </>\n                        :\n                            <ItemCount stock={stock} initial={1} onAdd={onAdd} />\n                    } \n                    \n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default ItemDetail\n","/home/mau/Escritorio/CursoReact/demo/src/components/Cart.js",[],"/home/mau/Escritorio/CursoReact/demo/src/context/cartContext.js",["88"],"import React, { createContext, useState, useEffect } from 'react'\n\nexport const CartContext = createContext()\n\nconst CartProvider = (props) => {\n\n    const [carro, setCarro] = useState([])\n    const [unidadesCarro, setUnidadesCarro] = useState(0)\n    const [totalCarro, setTotalCarro] = useState(0)\n\n    const addItem = (item, title, quantity, price, pictureUrl) => {        \n        if(isInCart(item).length === 0) {\n            setCarro([...carro, { id: item, title: title, quantity: quantity, precio: price, pictureUrl: pictureUrl, total: (price*quantity) }])\n        }\n        setUnidadesCarro(unidadesCarro + quantity)\n        setTotalCarro(totalCarro + (price*quantity))\n    }\n\n    const removeItem = (id, quantity) => {\n        let un = carro.filter(a => a.id === id)\n        setUnidadesCarro(unidadesCarro - un[0].quantity)\n        setTotalCarro(totalCarro - (un[0].precio * un[0].quantity))\n        const carrito = carro.filter(a => a.id !== id)\n        setCarro(carrito)\n    }\n\n    const clear = () => {\n        setCarro([])\n        setUnidadesCarro(0)\n        setTotalCarro(0)\n    }\n\n    const isInCart = (id) => {\n        let existe = carro.filter(a => a.id === id)\n        return existe\n    }\n\n     \n\n    return (\n        <CartContext.Provider\n            value= {{\n                carro,\n                setCarro,\n                addItem,\n                removeItem,\n                isInCart,\n                clear,\n                unidadesCarro,\n                totalCarro\n            }}\n        >\n            {props.children}\n        </CartContext.Provider>\n    )\n}\n\nexport default CartProvider\n","/home/mau/Escritorio/CursoReact/demo/src/components/DetalleItem.js",["89","90"],"import React, { useContext } from 'react'\nimport { NavLink, Navlink } from 'react-router-dom'\nimport {CartContext} from '../context/cartContext'\n\nfunction DetalleItem( { id, title, price, quantity, pictureUrl } ) {\n\n  const { removeItem } = useContext(CartContext)\n\n  const eliminarItem = () => {\n    removeItem(id)\n  }\n\n    return (\n          <div className=\"col-xs-12 col-sm-6 col-md-2 my-5 pb-5 text-center delete-padre\">\n            <button className=\"btn btn-dark btn-sm delete\"onClick={ eliminarItem } >\n              <span aria-hidden=\"true\">X</span>  \n            </button> \n            <NavLink to={`/item/`+ id} >\n                <img src={pictureUrl} className=\"img-fluid efecto  animate__animated animate__